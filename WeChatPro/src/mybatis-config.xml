<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <!--允许使用列别名-->
        <setting name="useColumnLabel" value="true"/>
        <!--支持主机自增生成策略-->
        <setting name="useGeneratedKeys" value="true"/>
        <!--开启驼峰标识-->
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>
    <typeAliases>
    <typeAlias alias="WechatUserHis" type="com.pulan.entity.WechatUserHis"/>
       <typeAlias alias="Message" type="com.pulan.entity.Message"/>
       <typeAlias alias="UserInfo" type="com.pulan.entity.UserInfo"/>
       <typeAlias alias="WechatValueBeh" type="com.pulan.entity.WechatValueBeh"/>
    </typeAliases>
  
  


</configuration>